CT_CONFIG_VERSION="3"
CT_ARCH_ARM=y
CT_ARCH_CPU="cortex-a73.cortex-a53"
CT_ARCH_64=y
CT_TARGET_CFLAGS="-march=armv8-a+crc+crypto -mabi=lp64 -Wno-psabi -mtune=cortex-a73.cortex-a53 -mno-outline-atomics"
CT_TARGET_LDFLAGS="-march=armv8-a+crc+crypto -mtune=cortex-a73.cortex-a53 -Wl,--as-needed -fuse-ld=gold"
CT_OMIT_TARGET_VENDOR=y
CT_KERNEL_LINUX=y
CT_BINUTILS_LINKER_LD_GOLD=y
CT_BINUTILS_GOLD_THREADS=y
CT_BINUTILS_LD_WRAPPER=y
CT_BINUTILS_PLUGINS=y
CT_GLIBC_V_2_32=y
CT_CC_LANG_CXX=y
