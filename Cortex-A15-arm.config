CT_CONFIG_VERSION="3"
CT_ARCH_ARM=y
CT_ARCH_CPU="cortex-a15"
CT_ARCH_SUFFIX="v7"
CT_ARCH_FPU="neon"
CT_TARGET_CFLAGS="-march=armv7-a -mtune=cortex-a15 -mfpu=neon -mfloat-abi=hard -Wno-psabi -Wa,-mno-warn-deprecated -Wall -pipe -O2 -fomit-frame-pointer"
CT_TARGET_LDFLAGS="-march=armv7-a -mtune=cortex-a15 -Wl,--hash-style=gnu -Wl,--as-needed -fuse-ld=gold"
CT_OMIT_TARGET_VENDOR=y
CT_KERNEL_LINUX=y
CT_LINUX_V_5_1=y
CT_BINUTILS_LINKER_LD_GOLD=y
CT_BINUTILS_GOLD_THREADS=y
CT_BINUTILS_LD_WRAPPER=y
CT_BINUTILS_PLUGINS=y
CT_GLIBC_V_2_32=y
CT_CC_LANG_CXX=y
